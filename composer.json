        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.9",
        "symfony/http-foundation" : "^4.4|^5.1",
        "symfony/http-kernel"  : "^4.4|^5.1",
        "symfony/event-dispatcher" : "^4.4|^5.1",
        "almasaeed2010/adminlte": "~2.3.0"
    },
    "require-dev": {
        "phpspec/prophecy": "^1.6"
